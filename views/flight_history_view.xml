<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.ui.view" id="view_historico_tree">
            <field name="name">flight.aircraft.history.securitytype</field>
            <field name="model">flight.aircraft.history.securitytype</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="create_uid"/>
                    <field name="create_date"/>
                    <field name="history_security_type_id"/>
                    <field name="history_change_radiogram"/>
                    <field name="history_security_observation"/>              
                   
                </tree>
            </field>
        </record>

        <record id="view_flight_history_form" model="ir.ui.view">
            <field name="name">flight.aircraft.history.securitytype</field>
            <field name="model">flight.aircraft.history.securitytype</field>
            <field name="arch" type="xml">
                <form string="NUEVO">
                    <table style="width:100%">
                        <tr>
                            <th>Firstname</th>
                            <th>Lastname</th>
                            <th>Age</th>
                        </tr>
                        <tr>
                            <td>Jill</td>
                            <td>Smith</td>
                            <td>50</td>
                        </tr>
                        <tr>
                        </tr>
                    </table>
                </form>
            </field>
        </record>



        <record model="ir.actions.act_window" id="action_historico">
            <field name="name">flight.aircraft.history.securitytype</field>
            <field name="res_model">flight.aircraft.history.securitytype</field>

            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Historico" id="menu_action_historico" parent="menu_flight_records" action="action_historico"/>


    </data>
</odoo>